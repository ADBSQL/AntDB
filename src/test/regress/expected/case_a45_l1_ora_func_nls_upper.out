set grammar to oracle;
select nls_upper('ab cDe') from dual;
 NLS_UPPER('AB CDE')  
----------------------
 AB CDE
(1 row)

select nls_upper('\?!@#$%^&*()sa') from dual;
 NLS_UPPER('\?!@#$%^&*()SA')  
------------------------------
 \?!@#$%^&*()SA
(1 row)

select nls_upper('aa\?!@#$%^&*()aa') from dual;
 NLS_UPPER('AA\?!@#$%^&*()AA')  
--------------------------------
 AA\?!@#$%^&*()AA
(1 row)

select nls_upper('\?!@#ss$%^&*()sC') from dual;
 NLS_UPPER('\?!@#SS$%^&*()SC')  
