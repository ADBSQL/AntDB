--日期	
create table tt(id number,dt date);
insert into tt values(1,to_date('2015-1-1','yyyy-mm-dd'));
insert into tt values(2,to_timestamp('2015-1-2 08:10:09','yyyy-mm-dd hh24:mi:ss'));
