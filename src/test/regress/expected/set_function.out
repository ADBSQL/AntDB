select * from tt order by id;
 id |         dt          
----+---------------------
  1 | 2015-01-01 00:00:00
  2 | 2015-01-02 08:10:09
  3 | 2015-01-02 09:10:09
(3 rows)

select min(dt) from tt;
         min         
---------------------
 2015-01-01 00:00:00
(1 row)

select max(dt) from tt;
         max         
---------------------
 2015-01-02 09:10:09
(1 row)

